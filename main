-- COSMETIC BUTTONS --




local Button = v4:CreateButton({
	Name = "No Face",
	Callback = function()
		for i,v in pairs(game.Players.LocalPlayer.Character.Head:GetChildren()) do
			if (v:IsA("Decal")) and v.Name == "Mad" or v.Name == "Stare" or v.Name == "chilled" or v.name == "Sad" then
			v:Destroy()
			end
			end
	end,
})
